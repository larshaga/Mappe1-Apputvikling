<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.larsh.mappe1.StatisticActivity">


    <TextView
        android:id="@+id/txts_NumberOfVictories"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/number_of_victorys"/>
    <TextView
        android:id="@+id/txte_NumberOfVictories"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/txts_NumberOfDefeats"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/number_of_defats"/>

    <TextView
        android:id="@+id/txte_NumberOfDefeats"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>


</LinearLayout>
